{
    "name": "nkoporec/social-ddev",
    "description": "Social ddev template for composer based Open Social projects.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "goalgorilla/open_social": "dev-feature/3162491-group-update-d9"
    },
    "require-dev": {
        "drush/drush": "10.*@stable",
        "palantirnet/drupal-rector" : "3248684.x-dev",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "~9.1.0",
        "drupal/devel": "^4.1",
        "dealerdirect/phpcodesniffer-composer-installer": "~0.7.1",
        "mglaman/phpstan-drupal": "^1.0",
        "mikey179/vfsstream": "~1.6.0",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "behat/behat": "3.*@stable",
        "behat/mink": "1.*@stable",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "drupal/drupal-extension": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\socialblue\\": "docroot/profiles/contrib/social/themes/socialblue/src",
            "Drupal\\socialbase\\": "docroot/profiles/contrib/social/themes/socialbase/src",
            "Drupal\\bootstrap\\": "docroot/themes/contrib/bootstrap/src"
        }
    },
    "repositories": {
        "rector-php": {
            "type": "vcs",
            "url": "https://github.com/bluehorndigital/drupal-rector.git"
        },
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": ["goalgorilla/open_social", "drupal/social"]
        },
        "1": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "social": {
            "type": "git",
            "url": "https://github.com/goalgorilla/open_social.git",
            "only": ["goalgorilla/open_social", "drupal/social"]
        }
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "drupal/core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/social": [
                "goalgorilla/open_social"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        }
    }
}
